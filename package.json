{
  "name": "fast-blake3",
  "version": "1.0.0",
  "description": "Fast BLAKE3 cryptographic hash function with TypeScript support",
  "type": "module",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc && cp src/blake3.js dist/esm/blake3.js",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "prepublishOnly": "npm run build",
    "test": "node test.js",
    "example": "node examples/example.js",
    "example:sdk": "node examples/sdk-example.js",
    "example:sdk:cjs": "node examples/sdk-example-cjs.cjs",
    "example:sdk:ts": "npx tsx examples/sdk-example.ts",
    "basic-hash": "node examples/basic-hash.js",
    "benchmark": "node benchmark.js"
  },
  "keywords": [
    "blake3",
    "hash",
    "cryptography",
    "simd",
    "wasm",
    "typescript",
    "crypto"
  ],
  "author": "Yeite Rilsosing Koireng",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.10.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
